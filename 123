            ValueBox.BackgroundColor3 = Colors.Background
            ValueBox.BorderSizePixel = 0
            ValueBox.Position = UDim2.new(0.6, 10, 0, 5)
            ValueBox.Size = UDim2.new(0.35, -25, 0, 25)
